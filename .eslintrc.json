{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        // "indent": [
        //     2,
        //     "tab"
        // ],
        "no-console": "off",
        "no-use-before-define": 0,
        "react/prop-types": ["error", { "ignore": ["navigation"] }],
        "import/prefer-default-export": 0
    }
}
